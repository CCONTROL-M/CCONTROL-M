[run]
source = app
omit =
    app/tests/*
    app/*/__pycache__/*
    app/*/tests/*
    app/*/migrations/*
    app/config/gunicorn.py
    app/config/asgi.py
    app/scripts/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    if TYPE_CHECKING:

[html]
directory = coverage_html 